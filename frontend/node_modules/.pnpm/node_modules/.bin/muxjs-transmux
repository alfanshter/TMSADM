#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/mux.js@6.3.0/node_modules/mux.js/bin/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/mux.js@6.3.0/node_modules/mux.js/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/mux.js@6.3.0/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/mux.js@6.3.0/node_modules/mux.js/bin/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/mux.js@6.3.0/node_modules/mux.js/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/mux.js@6.3.0/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mux.js/bin/transmux.js" "$@"
else
  exec node  "$basedir/../mux.js/bin/transmux.js" "$@"
fi
