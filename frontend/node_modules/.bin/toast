#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/toast@0.5.4_react@18.3.1/node_modules/toast/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/toast@0.5.4_react@18.3.1/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/toast@0.5.4_react@18.3.1/node_modules/toast/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/toast@0.5.4_react@18.3.1/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/NODE_OPTIONS='--experimental-loader="toast/src/loader.mjs"'" ]; then
  exec "$basedir/NODE_OPTIONS='--experimental-loader="toast/src/loader.mjs"'"  node "$basedir/../toast/toast-cli.mjs" "$@"
else
  exec NODE_OPTIONS='--experimental-loader="toast/src/loader.mjs"'  node "$basedir/../toast/toast-cli.mjs" "$@"
fi
