#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/esprima@https+++codeload.gi_3be95b6497ffbea8b30277eadbe3e6ac/node_modules/esprima/bin/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/esprima@https+++codeload.gi_3be95b6497ffbea8b30277eadbe3e6ac/node_modules/esprima/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/esprima@https+++codeload.gi_3be95b6497ffbea8b30277eadbe3e6ac/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/esprima@https+++codeload.gi_3be95b6497ffbea8b30277eadbe3e6ac/node_modules/esprima/bin/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/esprima@https+++codeload.gi_3be95b6497ffbea8b30277eadbe3e6ac/node_modules/esprima/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/esprima@https+++codeload.gi_3be95b6497ffbea8b30277eadbe3e6ac/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../bin/esvalidate.js" "$@"
fi
