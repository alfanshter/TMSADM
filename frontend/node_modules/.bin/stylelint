#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/stylelint@16.8.0_typescript@5.5.4/node_modules/stylelint/bin/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/stylelint@16.8.0_typescript@5.5.4/node_modules/stylelint/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/stylelint@16.8.0_typescript@5.5.4/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/stylelint@16.8.0_typescript@5.5.4/node_modules/stylelint/bin/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/stylelint@16.8.0_typescript@5.5.4/node_modules/stylelint/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/stylelint@16.8.0_typescript@5.5.4/node_modules:/mnt/c/laragon/www/TMSADM/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/stylelint@16.8.0_typescript@5.5.4/node_modules/stylelint/bin/stylelint.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/stylelint@16.8.0_typescript@5.5.4/node_modules/stylelint/bin/stylelint.mjs" "$@"
fi
