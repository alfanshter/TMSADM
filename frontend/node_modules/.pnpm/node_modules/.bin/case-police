#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/case-police@0.6.1/node_modules/case-police/bin/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/case-police@0.6.1/node_modules/case-police/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/case-police@0.6.1/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/case-police@0.6.1/node_modules/case-police/bin/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/case-police@0.6.1/node_modules/case-police/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/case-police@0.6.1/node_modules:/Users/macbook/Documents/Website/TMS-ADM/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../case-police/bin/case-police.mjs" "$@"
else
  exec node  "$basedir/../case-police/bin/case-police.mjs" "$@"
fi
